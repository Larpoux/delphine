			vscode.postMessage({ type: 'contentChanged', html, css });
		}, 150);
	}

	vscode.postMessage({ type: 'log', msg: 'A TES SOUHAITS' });


	editor.on('update', emitChangeDebounced);
	// ou des events plus ciblés si vous préférez :
	editor.on('component:add component:remove component:update', emitChangeDebounced);
	editor.on('style:property:update', emitChangeDebounced);    </script>
