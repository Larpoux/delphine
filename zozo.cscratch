	let isApplyingFromVscode = false;
	let pendingEmit = null;

	function emitChangeDebounced() {
		if (isApplyingFromVscode) return;

		clearTimeout(pendingEmit);
		pendingEmit = setTimeout(() => {
			if (isApplyingFromVscode) return; // re-check
			const html = editor.getComponents().getWrapper().toHTML();
			const css  = editor.getCss();
			//const html = editor.getHtml();
