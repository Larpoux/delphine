      let lastText = null;
      let lastCss = null;

     	function loadDocument({ html, css }) {
		if (html === lastText && css === lastCss) return;
		lastText = html;
		lastCss = css;

		isApplyingFromVscode = true;
			try {
			editor.DomComponents.clear();
			editor.CssComposer.clear();

			editor.setComponents(html);
			editor.setStyle(css);

			editor.select(null);
		} finally {
			// Important: laisser passer le micro-cycle GrapesJS qui pourrait encore déclencher des events
			setTimeout(() => { isApplyingFromVscode = false; }, 0);
		}
	}

	
 
	window.addEventListener('message', (event) => {
		const msg = event.data;
		if (msg?.type === 'update') {
		loadDocument({ html: msg.html ?? '', css: msg.css ?? '' });
		}
	});
		vscode.postMessage({ type: 'log', msg: 'A TES SOUHAITS' });

